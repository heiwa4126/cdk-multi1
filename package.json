{
	"name": "cdk-multi1",
	"version": "0.1.0",
	"bin": {
		"cdk-multi1": "bin/cdk-multi1.js"
	},
	"scripts": {
		"test": "jest",
		"format": "biome format --write . && dprint fmt && textlint --fix \"**/*.md\"",
		"check": "biome check --write . && dprint check",
		"bootstrap": "cdk bootstrap",
		"list": "cdk list",
		"synth": "cdk synth",
		"deploy:user1": "AWS_PROFILE=erai  cdk deploy User1Stack --outputs-file outputs1.tmp.json",
		"deploy:user2": "AWS_PROFILE=erai3 cdk deploy User2Stack --outputs-file outputs2.tmp.json",
		"deploy": "run-s deploy:user2 deploy:user1",
		"deploy:hotswap": "cdk deploy --hotswap --outputs-file outputs.tmp.json",
		"diff": "cdk diff",
		"destroy": "cdk destroy"
	},
	"devDependencies": {
		"@types/aws-lambda": "^8.10.146",
		"@types/jest": "^29.5.14",
		"@types/node": "22.7.9",
		"aws-cdk": "2.170.0",
		"esbuild": "^0.24.0",
		"jest": "^29.7.0",
		"npm-run-all": "^4.1.5",
		"ts-jest": "^29.2.5",
		"ts-node": "^10.9.2",
		"tsx": "^4.19.2",
		"typescript": "~5.6.3"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.700.0",
		"@aws-sdk/client-sts": "^3.699.0",
		"@aws-sdk/credential-providers": "^3.699.0",
		"aws-cdk-lib": "2.170.0",
		"constructs": "^10.4.2",
		"date-fns": "^4.1.0"
	}
}
